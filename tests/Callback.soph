func invokeLambda(lambda) {
    lambda.call()
}

func invokeIntLambda(lambda) {
    lambda.call(1)
}

func invokeCallback(&block) {
    yield
}

invokeLambda( () => {
    print("Hey!")
})

invokeIntLambda( (number : Int32) => {
    print("I got the number #{number}.")
})

invokeCallback() do
    print("Callback called!")
end

